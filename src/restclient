:url = http://localhost:8081

:headers = <<
Content-Type: application/json

#
# get current state of counter
#
GET :url/counter
:headers

{}

#
# Increment counter
#
POST :url/xlsx/query-builder/generate
:headers

{
    "msg": "Increment"
}



GET https://google.com

{}